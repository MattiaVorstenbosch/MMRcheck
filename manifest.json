{
  "manifest_version": 1,

  "name": "Dota 2 MMRcheck",
  "description": "A lightweight MMR checker for Dota 2",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
